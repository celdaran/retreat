Table scenario {
  scenario_id integer [increment, primary key]
  scenario_name varchar
  scenario_descr varchar
  account_type_id integer
  scenario_parent_id integer
  created_at timestamp [default: `now()`]
  modified_at timestamp [default: `now()`]
}

Table account_type {
  account_type_id integer [primary key]
  account_type_descr varchar
  created_at timestamp [default: `now()`]
  modified_at timestamp [default: `now()`]
}

Table expense {
  expense_id integer [increment, primary key]
  scenario_id integer
  expense_name varchar
  expense_descr varchar
  amount DECIMAL(13, 2)
  inflation_rate DECIMAL(5, 3)
  begin_year integer
  begin_month integer
  end_year integer
  end_month integer
  expense_status_id integer [not null]
  repeat_every integer
  created_at timestamp [default: `now()`]
  modified_at timestamp [default: `now()`]
}

Table expense_status {
  expense_status_id integer [primary key]
  expense_status_descr varchar
  created_at timestamp [default: `now()`]
  modified_at timestamp [default: `now()`]
}

Table asset {
  asset_id integer [increment, primary key]
  scenario_id integer
  asset_name varchar
  asset_descr varchar
  opening_balance DECIMAL(13, 2)
  current_balance DECIMAL(13, 2)
  max_withdrawal DECIMAL(13, 2)
  apr DECIMAL(5, 3)
  begin_after integer
  begin_year integer
  begin_month integer
  asset_status_id integer
  created_at timestamp [default: `now()`]
  modified_at timestamp [default: `now()`]
}

Table asset_status {
  asset_status_id integer [primary key]
  asset_status_descr varchar
  created_at timestamp [default: `now()`]
  modified_at timestamp [default: `now()`]
}


Ref: scenario.account_type_id > account_type.account_type_id

Ref: expense.scenario_id > scenario.scenario_id

Ref: expense.expense_status_id > expense_status.expense_status_id

Ref: asset.scenario_id > scenario.scenario_id

Ref: asset.asset_status_id > asset_status.asset_status_id

